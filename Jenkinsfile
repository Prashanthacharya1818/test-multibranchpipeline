pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        sh 'echo "hi hello build performance!"'
      }
    }
    stage('test') {
      steps {
        sh 'echo "hi hello test performance!"'
      }
    }
    stage('deploy') {
      steps {
        sh 'echo "hi hello deploy performance!"'
      }
    }
    stage('delivery') {
      steps {
        sh 'echo "hi hello delivery performance!"'
      }
    }
    stage('release') {
      steps {
        sh 'echo "hi hello release performance!"'
      }
    }
  }
}
      
